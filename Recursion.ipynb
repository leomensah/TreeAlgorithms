   "source": [
    "def stringReversal(string):\n",
    "    length = len(string)\n",
    "    if length == 0:\n",
    "        return \"\"\n",
    "    return stringReversal(string[1:]) + string[0]\n",
    "\n",
    "s = \"Thisismadfurry\"\n",
    "print(stringReversal(s))\n"
   ]
